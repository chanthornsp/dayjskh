{
  "name": "dayjskh",
  "version": "0.2.0",
  "type": "module",
  "description": "Khmer Lunar date Plugin for Day.js",
  "main": "./dist/dayjskh.umd.js",
  "module": "./dist/dayjskh.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/dayjskh.es.js",
      "require": "./dist/dayjskh.umd.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chanthornsp/dayjskh.git"
  },
  "author": "chanthorn",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.2",
    "prettier": "3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  }
}
